<!--pages/rookie/messege/detail.wxml-->
<view class='detail'>
<view class='text-blue text-bold title'>大学：</view>
	<view class="cu-form-group margin-top inputView " id="academy">
		<picker class="inputText" bindchange="PickerChange" value="{{index}}" range="{{picker}}" range-key="name">
			<view class="picker">
				{{index?picker[index].name:'选择大学'}}
			</view>
		</picker>
	</view>
	</view>

<view class='detail'>
<view class='text-blue text-bold title'>姓名：</view>
<view class="inputView"> 
  <input class="inputText" placeholder="请输入登陆码" value='{{}}' bindinput="" /> 
 </view>
</view>

<view class='detail'>
<view class='text-blue text-bold title'>出生年月：</view>
	<view class="cu-form-group inputView">
		<picker class="inputText" mode="date" value="{{date}}" start="2015-09-01" end="2020-09-01" bindchange="DateChange">
			<view class="picker ">
				{{date}}
			</view>
		</picker>
	</view>
  </view>

<view class='detail'>
<view class='text-blue text-bold title'>手机：</view>
<view class="inputView"> 
  <input class="inputText" placeholder="请输入手机" value='{{}}' bindinput=""  type='number'/> 
 </view>
</view>

<view class='detail'>
<view class='text-blue text-bold title'>学院：</view>
<view class="inputView"> 
  <input class="inputText" placeholder="请输入学院" value='{{}}' bindinput=""  type='number'/> 
 </view>
</view>

<view class='detail'>
<view class='text-blue text-bold title'>专业班级：</view>
<view class="inputView"> 
  <input class="inputText" placeholder="请输入专业班级" value='{{}}' bindinput=""  type='number'/> 
 </view>
</view>

<view class='detail'>
<view class='text-blue text-bold title'>学号：</view>
<view class="inputView"> 
  <input class="inputText" placeholder="请输入学号" value='{{}}' bindinput=""  type='number'/> 
 </view>
</view>

<view class='detail'>
<view class='text-blue text-bold title'>邮箱</view>
<view class="inputView"> 
  <input class="inputText" placeholder="请输入邮箱" value='{{}}' bindinput=""  type='number'/> 
 </view>
</view>

 <view class="loginBtnView"> 
  <button class="loginBtn" size="{{primarySize}}" loading="{{loading}}" plain="{{plain}}" disabled="{{disabled}}" bindtap="">确认</button> 
 </view>